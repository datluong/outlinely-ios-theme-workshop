<?xml version="1.0" encoding="UTF-8" ?>
<theme>
    <name>Maestro</name>
    <type>dark</type>
    <background-color>hsb(0,0,16)</background-color>
    <highlight-background-color>hsb(45,60,40)</highlight-background-color>
    <text-color>hsb(0,0,85)</text-color>
    <selection-color>hsb(45,60,40)</selection-color>
    <accent-color-one>hsb(45,80,84)</accent-color-one>
    <accent-color-two>hsb(45,80,84)</accent-color-two>
    <note-color>hsb(0,0,61)</note-color>
    <caret-color>hsb(45,88,84)</caret-color>
    <guide-color>hsb(45,80,40)</guide-color>
    <expander-color>hsb(45,80,84)</expander-color>
    <markup-color>hsb(45,80,84)</markup-color>
    <checkmark-color>hsb(45,80,84)</checkmark-color>
        <user-interface>
        <ui-general>
            <ui-general-divider-color>hsb(0,0,24)</ui-general-divider-color>
        </ui-general>
        <ui-navigation>
            <ui-navigation-tint-color>hsb(0,0,51)</ui-navigation-tint-color>
            <ui-navigation-bar-tint-color>hsb(0,0,10)</ui-navigation-bar-tint-color>
            <ui-navigation-bar-text-color>hsb(0,0,95)</ui-navigation-bar-text-color>
            <ui-navigation-bar-divider-color>hsb(0,0,23)</ui-navigation-bar-divider-color>
            <ui-navigation-modal-tint-color>hsb(0,0,10)</ui-navigation-modal-tint-color>
            <ui-navigation-textfield-background-color>hsb(0,0,25)</ui-navigation-textfield-background-color>
        </ui-navigation>
        <ui-table>
            <ui-table-header-background-color>hsb(0,0,10)</ui-table-header-background-color>
            <ui-table-header-text-color>hsb(0,0,95)</ui-table-header-text-color>
            <ui-table-divider-color>hsb(0,0,22)</ui-table-divider-color>
            <ui-table-background-color>hsb(0,0,16)</ui-table-background-color>
            <ui-table-cell-active-background-color>hsb({{hue}},80,84)</ui-table-cell-active-background-color>
            <ui-table-cell-idle-background-color>hsb(0,0,23)</ui-table-cell-idle-background-color>
            <ui-table-cell-text-color>hsb(0,0,91)</ui-table-cell-text-color>
            <ui-table-cell-subtitle-text-color>hsb(0,0,62)</ui-table-cell-subtitle-text-color>
            <ui-table-cell-tint-color>hsb({{hue}},80,84)</ui-table-cell-tint-color>
            <ui-table-highlight-background-color>hsba({{hue}},84,92,36)</ui-table-highlight-background-color>
            <ui-table-highlight-text-color>hsb({{hue}},60,80)</ui-table-highlight-text-color>
        </ui-table>
        <ui-sidebar>
            <ui-sidebar-is-vibrant>false</ui-sidebar-is-vibrant>
            <ui-sidebar-tint-color>hsb(0,0,51)</ui-sidebar-tint-color>
            <ui-sidebar-active-tint-color>hsb({{hue}},80,84)</ui-sidebar-active-tint-color>
            <ui-sidebar-background-color>hsb(0,0,16)</ui-sidebar-background-color>
            <ui-sidebar-cell-active-background-color>hsb(0,0,21)</ui-sidebar-cell-active-background-color>
            <ui-sidebar-header-text-color>hsb(0,0,68)</ui-sidebar-header-text-color>
            <ui-sidebar-cell-text-color>hsb(0,0,91)</ui-sidebar-cell-text-color>
            <ui-sidebar-token-color>hsb(0,0,20)</ui-sidebar-token-color>
            <ui-sidebar-token-text-color>hsb(0,0,95)</ui-sidebar-token-text-color>
        </ui-sidebar>
        <ui-dialog>
            <ui-dialog-background-color>hsb(0,0,20)</ui-dialog-background-color>
            <ui-dialog-alt-background-color>hsb(0,0,30)</ui-dialog-alt-background-color>
            <ui-dialog-text-color>hsb(0,0,91)</ui-dialog-text-color>
            <ui-dialog-alt-text-color>hsb(0,0,75)</ui-dialog-alt-text-color>
            <ui-dialog-subtitle-text-color>hsb(0,0,61)</ui-dialog-subtitle-text-color>
            <ui-dialog-alt-highlight-text-color>hsb({{hue}},88,88)</ui-dialog-alt-highlight-text-color>
            <ui-dialog-alt-highlight-background-color>hsba({{hue}},30,96,36)</ui-dialog-alt-highlight-background-color>
        </ui-dialog>
    </user-interface>
</theme>
