<?xml version="1.0" encoding="UTF-8" ?>
<theme>
    <name>Zenburn</name>
    <type>dark</type>
    <background-color>#3f3f3f</background-color>
    <highlight-background-color>#70672D</highlight-background-color>
    <selection-color>#666666</selection-color>
    <text-color>#dcdccc</text-color>
    <accent-color-one>#cc9393</accent-color-one>
    <accent-color-two>#8cd0d2</accent-color-two>
    <note-color>#73736d</note-color>
    <guide-color>#4b4b4a</guide-color>
    <expander-color>#73736d</expander-color>
    <caret-color>8faf9f</caret-color>
    <markup-color>hsb(61,18,84)</markup-color>
    <user-interface>
        <ui-general>
            <ui-general-divider-color>hsb(0,0,12)</ui-general-divider-color>
        </ui-general>
        <ui-navigation>
            <ui-navigation-tint-color>hsb(29,28,69)</ui-navigation-tint-color>
            <ui-navigation-bar-tint-color>#393939</ui-navigation-bar-tint-color>
            <ui-navigation-bar-text-color>#cccccc</ui-navigation-bar-text-color>
            <ui-navigation-bar-divider-color>hsb(0,0,12)</ui-navigation-bar-divider-color>
            <ui-navigation-modal-tint-color>#393939</ui-navigation-modal-tint-color>
            <ui-navigation-textfield-background-color>#4D4D4D</ui-navigation-textfield-background-color>
        </ui-navigation>
        <ui-table>
            <ui-table-header-background-color>#393939</ui-table-header-background-color>
            <ui-table-header-text-color>#D6D7AF</ui-table-header-text-color>
            <ui-table-divider-color>hsb(0,0,15)</ui-table-divider-color>
            <ui-table-background-color>#3f3f3f</ui-table-background-color>
            <ui-table-cell-active-background-color>#5C5C58</ui-table-cell-active-background-color>
            <ui-table-cell-idle-background-color>#474747</ui-table-cell-idle-background-color>
            <ui-table-cell-text-color>#D6D7AF</ui-table-cell-text-color>
            <ui-table-cell-subtitle-text-color>#B8B8AE</ui-table-cell-subtitle-text-color>
            <ui-table-selection-text-color>#D6D7AF</ui-table-selection-text-color>
            <ui-table-cell-tint-color>#B19880</ui-table-cell-tint-color>
            <ui-table-highlight-background-color>hsba(61,20,50,25)</ui-table-highlight-background-color>
            <ui-table-highlight-text-color>#FFFC96</ui-table-highlight-text-color>
        </ui-table>
        <ui-sidebar>
            <ui-sidebar-is-vibrant>false</ui-sidebar-is-vibrant>
            <ui-sidebar-tint-color>#B19880</ui-sidebar-tint-color>
            <ui-sidebar-active-tint-color>#CCAC8D</ui-sidebar-active-tint-color>
            <ui-sidebar-image-active-tint-color>#CCAC8D</ui-sidebar-image-active-tint-color>
            <ui-sidebar-background-color>#393939</ui-sidebar-background-color>
            <ui-sidebar-cell-active-background-color>#474747</ui-sidebar-cell-active-background-color>
            <ui-sidebar-header-text-color>#898989</ui-sidebar-header-text-color>
            <ui-sidebar-cell-text-color>#D9D9D9</ui-sidebar-cell-text-color>
            <ui-sidebar-token-color>#5C5C5C</ui-sidebar-token-color>
            <ui-sidebar-token-text-color>#CCCCCC</ui-sidebar-token-text-color>
        </ui-sidebar>
        <ui-dialog>
            <ui-dialog-is-unique>true</ui-dialog-is-unique>
            <ui-dialog-background-color>#444444</ui-dialog-background-color>
            <ui-dialog-alt-background-color>#444444</ui-dialog-alt-background-color>
            <ui-dialog-text-color>#D6D7AF</ui-dialog-text-color>
            <ui-dialog-alt-text-color>#999991</ui-dialog-alt-text-color>
            <ui-dialog-selection-text-color>#D6D7AF</ui-dialog-selection-text-color>
            <ui-dialog-selection-alt-text-color>#999991</ui-dialog-selection-alt-text-color>
            <ui-dialog-subtitle-text-color>#8C8C85</ui-dialog-subtitle-text-color>
            <ui-dialog-alt-selection-background-color>#91C0C2</ui-dialog-alt-selection-background-color>
            <ui-dialog-alt-highlight-text-color>#3f3f3f</ui-dialog-alt-highlight-text-color>
            <ui-dialog-alt-highlight-background-color>hsba(182,50,76,25)</ui-dialog-alt-highlight-background-color>
            <ui-dialog-search-indicator-color>#8CD0D2</ui-dialog-search-indicator-color>
        </ui-dialog>
    </user-interface>
</theme>
