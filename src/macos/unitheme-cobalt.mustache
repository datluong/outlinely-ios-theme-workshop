<?xml version="1.0" encoding="UTF-8" ?>
<theme>
    <name>Cobalt</name>
    <type>dark</type>
    <background-color>#193549</background-color>
    <highlight-background-color>#6B6436</highlight-background-color>
    <search-background-color>#8B76F5</search-background-color>
    <text-color>#FFFFFF</text-color>
    <selection-color>#0050A4</selection-color>
    <accent-color-one>#3AD900</accent-color-one>
    <accent-color-two>#FF9D00</accent-color-two>
    <note-color>#0088FF</note-color>
    <caret-color>#FFC600</caret-color>
    <guide-color>#3B5364</guide-color>
    <expander-color>#5A7B92</expander-color>
    <markup-color>#FFC700</markup-color>
    <checkmark-color>#3AD900</checkmark-color>
    <gutter-text-color>#8B9AA4</gutter-text-color>
    <title-color>#FFC700</title-color>
    <user-interface>
        <ui-general>
            <ui-general-divider-color>hsb(205,53,10)</ui-general-divider-color>
        </ui-general>
        <ui-navigation>
            <ui-navigation-tint-color>hsb(104,100,85)</ui-navigation-tint-color>
            <ui-navigation-bar-tint-color>hsb(205,66,29)</ui-navigation-bar-tint-color>
            <ui-navigation-bar-text-color>hsb(0,0,95)</ui-navigation-bar-text-color>
            <ui-navigation-bar-divider-color>hsb(205,53,10)</ui-navigation-bar-divider-color>
            <ui-navigation-modal-tint-color>hsb(205,66,29)</ui-navigation-modal-tint-color>
            <ui-navigation-textfield-background-color>hsb(205,41,39)</ui-navigation-textfield-background-color>
        </ui-navigation>
        <ui-table>
            <ui-table-header-background-color>hsb(205,66,29)</ui-table-header-background-color>
            <ui-table-header-text-color>hsb(0,0,95)</ui-table-header-text-color>
            <ui-table-divider-color>hsb(205,53,12)</ui-table-divider-color>
            <ui-table-background-color>hsb(205,66,29)</ui-table-background-color>
            <ui-table-cell-active-background-color>hsb(205,71,37)</ui-table-cell-active-background-color>
            <ui-table-cell-idle-background-color>hsb(205,31,31)</ui-table-cell-idle-background-color>
            <ui-table-cell-text-color>hsb(47,100,100)</ui-table-cell-text-color>
            <ui-table-cell-subtitle-text-color>hsb(205,15,64)</ui-table-cell-subtitle-text-color>
            <ui-table-cell-tint-color>hsb(47,100,100)</ui-table-cell-tint-color>
            <ui-table-highlight-background-color>hsba(148,52,100,25)</ui-table-highlight-background-color>
            <ui-table-highlight-text-color>hsb(47,100,100)</ui-table-highlight-text-color>
        </ui-table>
        <ui-sidebar>
            <ui-sidebar-is-vibrant>false</ui-sidebar-is-vibrant>
            <ui-sidebar-tint-color>hsb(47,100,100)</ui-sidebar-tint-color>
            <ui-sidebar-active-tint-color>hsb(37,100,100)</ui-sidebar-active-tint-color>
            <ui-sidebar-image-active-tint-color>hsb(37,100,100)</ui-sidebar-image-active-tint-color>
            <ui-sidebar-background-color>hsb(205,68,18)</ui-sidebar-background-color>
            <ui-sidebar-cell-active-background-color>hsb(205,33,31)</ui-sidebar-cell-active-background-color>
            <ui-sidebar-header-text-color>hsb(205,15,64)</ui-sidebar-header-text-color>
            <ui-sidebar-cell-text-color>hsb(0,0,95)</ui-sidebar-cell-text-color>
            <ui-sidebar-token-color>hsb(205,33,31)</ui-sidebar-token-color>
            <ui-sidebar-token-text-color>hsb(0,0,95)</ui-sidebar-token-text-color>
        </ui-sidebar>
        <ui-dialog>
            <ui-dialog-is-unique>true</ui-dialog-is-unique>
            <ui-dialog-background-color>hsb(205,53,26)</ui-dialog-background-color>
            <ui-dialog-alt-background-color>hsb(205,53,18)</ui-dialog-alt-background-color>
            <ui-dialog-text-color>hsb(47,100,100)</ui-dialog-text-color>
            <ui-dialog-alt-text-color>hsb(205,15,64)</ui-dialog-alt-text-color>
            <ui-dialog-selection-text-color>hsb(47,100,100)</ui-dialog-selection-text-color>
            <ui-dialog-selection-alt-text-color>hsb(205,15,64)</ui-dialog-selection-alt-text-color>
            <ui-dialog-subtitle-text-color>hsb(205,15,55)</ui-dialog-subtitle-text-color>
            <ui-dialog-alt-selection-background-color>hsb(47,100,100)</ui-dialog-alt-selection-background-color>
            <ui-dialog-alt-highlight-text-color>hsb(37,100,100)</ui-dialog-alt-highlight-text-color>
            <ui-dialog-alt-highlight-background-color>hsba(47,50,100,25)</ui-dialog-alt-highlight-background-color>
            <ui-dialog-search-indicator-color>#7BC6F1</ui-dialog-search-indicator-color>
        </ui-dialog>
    </user-interface>
</theme>
