<?xml version="1.0" encoding="UTF-8" ?>
<theme>
    <name>Thinkspace Ocean</name>
    <type>light</type>
    <background-color>#fbfbfb</background-color>
    <text-color>#464646</text-color>
    <selection-color>hsb(188,27,96)</selection-color>
    <accent-color-one>hsb(188,91,80)</accent-color-one>
    <accent-color-two>hsb(188,91,80)</accent-color-two>
    <note-color>#808080</note-color>
    <caret-color>hsb(193,61,81)</caret-color>
    <guide-color>hsb(188,27,95)</guide-color>
    <expander-color>hsb(188,91,80)</expander-color>
    <handle-bar-color>hsb(0,0,90)</handle-bar-color>
    <markup-color>hsb(188,91,80)</markup-color>
    <checkmark-color>hsb(188,91,80)</checkmark-color>
    <user-interface>
        <ui-general>
          <ui-general-divider-color>hsb(188,2,85)</ui-general-divider-color>
        </ui-general>
        <ui-toolbar>
          <ui-toolbar-image-color>#464646</ui-toolbar-image-color>
          <ui-toolbar-tint-color>hsb(188,91,80)</ui-toolbar-tint-color>
          <ui-toolbar-background-color>#fcfcfc</ui-toolbar-background-color>
        </ui-toolbar>
        <ui-navigation>
          <ui-navigation-bar-tint-color>#fafafa</ui-navigation-bar-tint-color>
          <ui-navigation-bar-text-color>#000000</ui-navigation-bar-text-color>
          <ui-navigation-bar-divider-color>hsb(188,2,85)</ui-navigation-bar-divider-color>
          <ui-navigation-tint-color>hsb(188,91,80)</ui-navigation-tint-color>
          <ui-navigation-modal-tint-color>hsb(0,0,98)</ui-navigation-modal-tint-color>
          <ui-navigation-textfield-background-color>hsb(0,0,92)</ui-navigation-textfield-background-color>
        </ui-navigation>
        <ui-table>
            <ui-table-header-background-color>hsb(188,2,96)</ui-table-header-background-color>
            <ui-table-header-text-color>#000000</ui-table-header-text-color>
            <ui-table-divider-color>hsb(188,2,88)</ui-table-divider-color>
            <ui-table-background-color>#fbfbfb</ui-table-background-color>
            <ui-table-cell-active-background-color>hsb(188,84,84)</ui-table-cell-active-background-color>
            <ui-table-cell-idle-background-color>hsb(188,2,76)</ui-table-cell-idle-background-color>
            <ui-table-cell-text-color>#454545</ui-table-cell-text-color>
            <ui-table-cell-subtitle-text-color>#808080</ui-table-cell-subtitle-text-color>
            <ui-table-cell-tint-color>hsb(188,84,89)</ui-table-cell-tint-color>
            <ui-table-highlight-background-color>hsb(188,22,96)</ui-table-highlight-background-color>
            <ui-table-highlight-text-color>hsb(188,84,80)</ui-table-highlight-text-color>
        </ui-table>
    </user-interface>
</theme>
