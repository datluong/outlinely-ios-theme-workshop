<?xml version="1.0" encoding="UTF-8" ?>
<theme>
    <name>Solarized Light</name>
    <type>light</type>
    <background-color>#fdf6e3</background-color>
    <highlight-background-color>#FFE366</highlight-background-color>
    <selection-color>#EEE8D5</selection-color>
    <text-color>#586e75</text-color>
    <accent-color-one>#268bd2</accent-color-one>
    <accent-color-two>#b58900</accent-color-two>
    <note-color>#93A1A1</note-color>
    <guide-color>#e7e5d4</guide-color>
    <expander-color>#93A1A1</expander-color>
    <caret-color>#647b84</caret-color>
    <user-interface>
        <ui-general>
            <ui-general-divider-color>hsb(46,11,80)</ui-general-divider-color>
        </ui-general>
        <ui-navigation>
            <ui-navigation-tint-color>hsb(196,23,51)</ui-navigation-tint-color>
            <ui-navigation-active-tint-color>#b58900</ui-navigation-active-tint-color>
            <ui-navigation-bar-tint-color>hsb(46,11,97)</ui-navigation-bar-tint-color>
            <ui-navigation-bar-text-color>hsb(196,23,51)</ui-navigation-bar-text-color>
            <ui-navigation-bar-divider-color>hsb(46,11,80)</ui-navigation-bar-divider-color>
            <ui-navigation-modal-tint-color>hsb(46,11,97)</ui-navigation-modal-tint-color>
            <ui-navigation-textfield-background-color>hsb(46,10,99)</ui-navigation-textfield-background-color>
        </ui-navigation>
        <ui-table>
            <ui-table-header-background-color>#FAF3DE</ui-table-header-background-color>
            <ui-table-header-text-color>hsb(196,89,27)</ui-table-header-text-color>
            <ui-table-divider-color>hsb(46,11,87)</ui-table-divider-color>
            <ui-table-background-color>#FAF3DE</ui-table-background-color>
            <ui-table-cell-active-background-color>hsb(196,23,51)</ui-table-cell-active-background-color>
            <ui-table-cell-idle-background-color>hsb(180,9,60)</ui-table-cell-idle-background-color>
            <ui-table-cell-text-color>hsb(196,89,27)</ui-table-cell-text-color>
            <ui-table-cell-subtitle-text-color>hsb(186,13,59)</ui-table-cell-subtitle-text-color>
            <ui-table-cell-tint-color>hsb(186,13,59)</ui-table-cell-tint-color>
            <ui-table-highlight-background-color>hsba(180,9,63,25)</ui-table-highlight-background-color>
            <ui-table-highlight-text-color>hsb(196,25,46)</ui-table-highlight-text-color>
        </ui-table>
        <ui-sidebar>
            <ui-sidebar-is-vibrant>false</ui-sidebar-is-vibrant>
            <ui-sidebar-tint-color>hsb(180,9,63)</ui-sidebar-tint-color>
            <ui-sidebar-active-tint-color>hsb(46,10,99)</ui-sidebar-active-tint-color>
            <ui-sidebar-image-active-tint-color>hsb(46,10,99)</ui-sidebar-image-active-tint-color>
            <ui-sidebar-background-color>hsb(196,89,32)</ui-sidebar-background-color>
            <ui-sidebar-cell-active-background-color>hsb(196,25,46)</ui-sidebar-cell-active-background-color>
            <ui-sidebar-header-text-color>hsb(180,9,63)</ui-sidebar-header-text-color>
            <ui-sidebar-cell-text-color>hsb(46,10,99)</ui-sidebar-cell-text-color>
            <ui-sidebar-token-color>hsb(196,25,46)</ui-sidebar-token-color>
            <ui-sidebar-token-text-color>hsb(46,11,93)</ui-sidebar-token-text-color>
            <ui-sidebar-divider-color>hsb(196,89,32)</ui-sidebar-divider-color>
        </ui-sidebar>
        <ui-dialog>
            <ui-dialog-is-unique>true</ui-dialog-is-unique>
            <ui-dialog-background-color>hsb(46,10,99)</ui-dialog-background-color>
            <ui-dialog-alt-background-color>hsb(46,11,100)</ui-dialog-alt-background-color>
            <ui-dialog-text-color>hsb(192,100,21)</ui-dialog-text-color>
            <ui-dialog-alt-text-color>hsb(194,25,46)</ui-dialog-alt-text-color>
            <ui-dialog-subtitle-text-color>hsb(180,9,63)</ui-dialog-subtitle-text-color>
            <ui-dialog-selection-text-color>#fdf6e3</ui-dialog-selection-text-color>
            <ui-dialog-selection-subtitle-text-color>hsba(44,10,99,60)</ui-dialog-selection-subtitle-text-color>
            <ui-dialog-alt-selection-background-color>#268bd2</ui-dialog-alt-selection-background-color>
            <ui-dialog-alt-highlight-text-color>hsb(196,25,46)</ui-dialog-alt-highlight-text-color>
            <ui-dialog-alt-highlight-background-color>hsba(180,9,63,25)</ui-dialog-alt-highlight-background-color>
        </ui-dialog>
    </user-interface>
</theme>
