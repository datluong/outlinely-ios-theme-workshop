<?xml version="1.0" encoding="UTF-8" ?>
<theme>
    <name>Darkdev</name>
    <type>dark</type>
    <background-color>#1a1a1a</background-color>
    <text-color>#cccccc</text-color>
    <selection-color>#4a4a4a</selection-color>
    <accent-color-one>#63caf0</accent-color-one>
    <accent-color-two>#63caf0</accent-color-two>
    <note-color>#9c9c9c</note-color>
    <guide-color>#363636</guide-color>
    <expander-color>#686868</expander-color>
    <markup-color>#686868</markup-color>
    <checkmark-color>#686868</checkmark-color>
    <user-interface>
        <ui-general>
            <ui-general-divider-color>#686868</ui-general-divider-color>
        </ui-general>
        <ui-navigation>
            <ui-navigation-tint-color>#1DB7F0</ui-navigation-tint-color>
            <ui-navigation-bar-tint-color>#363636</ui-navigation-bar-tint-color>
            <ui-navigation-bar-text-color>#f2f2f2</ui-navigation-bar-text-color>
            <ui-navigation-bar-divider-color>#686868</ui-navigation-bar-divider-color>
        </ui-navigation>
        <ui-table>
            <ui-table-header-background-color>#2e2e2e</ui-table-header-background-color>
            <ui-table-header-text-color>#e8e8e8</ui-table-header-text-color>
            <ui-table-divider-color>#525252</ui-table-divider-color>
            <ui-table-background-color>#1c1c1c</ui-table-background-color>
            <ui-table-cell-active-background-color>hsb(196,87,94)</ui-table-cell-active-background-color>
            <ui-table-cell-idle-background-color>#3d3d3d</ui-table-cell-idle-background-color>
            <ui-table-cell-text-color>#e8e8e8</ui-table-cell-text-color>
            <ui-table-cell-subtitle-text-color>#cccccc</ui-table-cell-subtitle-text-color>
            <ui-table-cell-tint-color>#1DB7F0</ui-table-cell-tint-color>
        </ui-table>
    </user-interface>
</theme>

