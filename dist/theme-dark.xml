<?xml version="1.0" encoding="UTF-8" ?>
<theme>
    <name>Dark</name>
    <type>dark</type>
    <background-color>#1a1a1a</background-color>
    <highlight-background-color>#705c2f</highlight-background-color>
    <text-color>#cccccc</text-color>
    <selection-color>#4a4a4a</selection-color>
    <accent-color-one>#63caf0</accent-color-one>
    <accent-color-two>#63caf0</accent-color-two>
    <note-color>#9c9c9c</note-color>
    <guide-color>#363636</guide-color>
    <expander-color>#686868</expander-color>
    <handle-bar-color>hsb(0,0,20)</handle-bar-color>
    <markup-color>#686868</markup-color>
    <checkmark-color>#686868</checkmark-color>
    <user-interface>
        <ui-general>
            <ui-general-divider-color>hsb(0,0,25)</ui-general-divider-color>
        </ui-general>
        <ui-toolbar>
          <ui-toolbar-image-color>#f2f2f2</ui-toolbar-image-color>
          <ui-toolbar-tint-color>#1DA9F0</ui-toolbar-tint-color>
          <ui-toolbar-background-color>#1c1c1c</ui-toolbar-background-color>
        </ui-toolbar>
        <ui-navigation>
            <ui-navigation-tint-color>#1DA9F0</ui-navigation-tint-color>
            <ui-navigation-bar-tint-color>hsb(0,0,6)</ui-navigation-bar-tint-color>
            <ui-navigation-bar-text-color>#f2f2f2</ui-navigation-bar-text-color>
            <ui-navigation-bar-divider-color>hsb(0,0,25)</ui-navigation-bar-divider-color>
            <ui-navigation-modal-tint-color>hsb(0,0,8)</ui-navigation-modal-tint-color>
            <ui-navigation-textfield-background-color>hsb(0,0,26)</ui-navigation-textfield-background-color>
        </ui-navigation>
        <ui-table>
            <ui-table-header-background-color>hsb(0,0,16)</ui-table-header-background-color>
            <ui-table-header-text-color>hsb(0,0,91)</ui-table-header-text-color>
            <ui-table-divider-color>hsb(0,0,20)</ui-table-divider-color>
            <ui-table-background-color>#1c1c1c</ui-table-background-color>
            <ui-table-cell-active-background-color>hsb(200,82,86)</ui-table-cell-active-background-color>
            <ui-table-cell-idle-background-color>hsb(0,0,25)</ui-table-cell-idle-background-color>
            <ui-table-cell-text-color>#e8e8e8</ui-table-cell-text-color>
            <ui-table-cell-subtitle-text-color>#cccccc</ui-table-cell-subtitle-text-color>
            <ui-table-cell-tint-color>#1DA9F0</ui-table-cell-tint-color>
            <ui-table-highlight-background-color>hsb(200,46,28)</ui-table-highlight-background-color>
            <ui-table-highlight-text-color>hsb(200,64,88)</ui-table-highlight-text-color>
        </ui-table>
        <ui-dialog>
            <ui-dialog-background-color>hsb(0,0,32)</ui-dialog-background-color>
            <ui-dialog-alt-background-color>hsb(0,0,32)</ui-dialog-alt-background-color>
            <ui-dialog-text-color>hsb(0,0,95)</ui-dialog-text-color>
            <ui-dialog-alt-text-color>hsb(0,0,75)</ui-dialog-alt-text-color>
            <ui-dialog-subtitle-text-color>hsb(0,0,60)</ui-dialog-subtitle-text-color>
            <ui-dialog-selection-text-color>white</ui-dialog-selection-text-color>
            <ui-dialog-selection-subtitle-text-color>hsba(0,0,100,60)</ui-dialog-selection-subtitle-text-color>
            <ui-dialog-selection-alt-text-color>hsba(0,0,100,60)</ui-dialog-selection-alt-text-color>
        </ui-dialog>
    </user-interface>
</theme>
